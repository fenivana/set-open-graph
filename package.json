{
  "name": "set-open-graph",
  "version": "0.1.2",
  "description": "Setting Open Graph meta tags of the html",
  "main": "dist/OpenGraph.js",
  "module": "dist/OpenGraph.mjs",
  "scripts": {
    "build": "rollup -c rollup.config.esm.mjs && rollup -c rollup.config.umd.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fenivana/set-open-graph.git"
  },
  "keywords": [
    "open graph",
    "meta"
  ],
  "author": "Jiang Fengming",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fenivana/set-open-graph/issues"
  },
  "homepage": "https://github.com/fenivana/set-open-graph#readme",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "eslint": "^5.4.0",
    "eslint-config-enough": "^0.3.4",
    "rollup": "^0.65.0",
    "rollup-plugin-babel": "^4.0.2"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "loose": true,
          "modules": false
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "enough",
    "env": {
      "browser": true
    }
  }
}
